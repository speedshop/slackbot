{
  "name": "slackbot",
  "description": "A Slack bot application",
  "repository": "https://github.com/speedshop/slackbot",
  "env": {
    "NODE_ENV": {
      "description": "The environment to run the application in",
      "value": "production"
    }
  },
  "docker": {
    "dockerfile": "../Dockerfile"
  },
  "healthchecks": {
    "web": [
        {
            "type":        "uptime",
            "name":        "is it running",
            "description": "up for 10 seconds or no",
            "uptime":      10
        }
    ]
  }
}
